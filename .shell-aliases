alias kc=kubectl
alias tf=terraform
alias dotfiles='git --git-dir=$HOME/.dotfiles-bare-repo/ --work-tree=$HOME/'
alias .f=dotfiles

function inc-install() { 
  npx google-artifactregistry-auth ~/.npmrc
  npm install $@
}

alias cdev='cd ~/Dev'
alias docko=docker-compose

function b64D() {
  echo $1 | base64 --decode
}
